{
    "name": "DeepForge (TensorFlow)",
    "build": {
        "dockerfile": "../../Dockerfile.tensorflow"
    },
    "runArgs": [
        "--gpus",
        "all",
        "--ipc=host",
        "--ulimit",
        "memlock=-1",
        "--ulimit",
        "stack=67108864",
        "--env-file",
        ".env"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "njpwerner.autodocstring",
            ]
        }
    }
}